  "realEstateForm": {
    "formId": "realEstateForm",
    "actions": [
      {
        "host": "lightapi.net",
        "service": "covid",
        "action": "updateWebsite",
        "version": "0.1.0",
        "title": "For Rent or Sale",
        "success": "/app/success",
        "failure": "/app/failure"
      }
    ],
    "schema": {
      "title": "Real Estate For Rent or For Sale",
      "type": "object",
      "properties": {
        "co": {
          "title": "Component",
          "type": "string",
          "default": "RealEstateComponent",
          "readonly": true
        },
        "ss": {
          "type": "array",
          "title": "Properties",
          "minItems": 1,
          "maxItems": 10,
          "items": {
            "type": "object",
            "properties": {
              "t": {
                "title": "Timestamp",
                "type": "string"
              },
              "s": {
                "title": "Transaction",
                "type": "string",
                "enum": [
                  "Rent",
                  "Sale"
                ]
              },
              "d": {
                "title": "Description",
                "type": "string"
              },
              "address": {
                "type": "string"
              },
              "postalCode": {
                "type": "string"
              },
              "price": {
                "type": "string"
              },
              "propertyType": {
                "type": "string",
                "default": "Single Family",
                "enum": [
                  "Single Family",
                  "Recreational",
                  "Condominium",
                  "Multi Family",
                  "Agriculture",
                  "Parking",
                  "Vacant Land"
                ] 
              },
              "buildingType": {
                "type": "string",
                "default": "House",
                "enum": [
                  "House",
                  "Apartment",
                  "Townhouse",
                  "Duplex",
                  "Triplex",
                  "Fourplex",
                  "Mobile Home",
                  "Sepcial Purpose"
                ]
              },
              "communityName": {
                "type": "string"
              },
              "title": {
                "type": "string",
                "default": "Freehold",
                "enum": [
                  "Freehold",
                  "Leasehold",
                  "Strata",
                  "Crosslease"
                ]
              },
              "is": {
                "type": "array",
                "title": "Optional Image Urls",
                "maxItems": 50,
                "items": {
                  "type": "object",
                  "properties": {
                    "u": {
                      "title": "Image Url",
                      "type": "string"
                    }
                  },
                  "required": [
                    "u"
                  ]
                } 
              },
              "vs": {
                "type": "array",
                "title": "Optional Video Urls",
                "maxItems": 5,
                "items": {
                  "type": "object",
                  "properties": {
                    "u": {
                      "title": "Video Url",
                      "type": "string"
                    }
                  },
                  "required": [
                    "u"
                  ]
                } 
              },
              "storeys": {
                "type": "string"
              },
              "landSize": {
                "type": "string"
              },
              "aboveGradeBedrooms": {
                "type": "string"
              },
              "belowGradeBedrooms": {
                "type": "string"
              },
              "bathrooms": {
                "type": "string"
              },
              "basementType": {
                "type": "string"
              },
              "buildingFeatures": {
                "type": "string"
              },
              "buildingStyle": {
                "type": "string"
              },
              "cooling": {
                "type": "string"
              },
              "heating": {
                "type": "string"
              },
              "exteriorFinish": {
                "type": "string"
              },
              "amenitiesNearby": {
                "type": "string"
              },
              "parkingType": {
                "type": "string",
                "default": "Attached",
                "enum": [
                  "Attached",
                  "Detached",
                  "Underground",
                  "Visitor"
                ]
              },
              "totalParkingSpaces": {
                "type": "string"
              }
            },
            "required": [
              "t",
              "s",
              "d",
              "address",
              "postalCode",
              "price",
              "propertyType",
              "buildingType",
              "communityName",
              "title"
            ]
          }
        }  
      },
      "required": [
        "co",
        "ss"
      ]
    },
    "form": [
      "co",
      {
        "key": "ss",
        "add": "New",
        "style": {
          "add": "btn-success"
        },
        "items": [
         {
           "key": "ss[].t",
           "type": "timestamp"
          },
          "ss[].s",
          {
            "key": "ss[].d",
            "type": "textarea"
          },
          "ss[].address",
          "ss[].postalCode",
          "ss[].price",
          "ss[].propertyType",
          "ss[].buildingType",
          "ss[].communityName",
          "ss[].title",
          {
            "key": "ss[].is",
